{
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "eslint.useFlatConfig": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "files.exclude": {
    "**/node_modules": true
  },
  "search.exclude": {},
  "tailwindCSS.experimental.configFile": {
    "src/styles/app.css": "src/**"
  },
  "typescript.tsdk": "node_modules/typescript/lib"
}
