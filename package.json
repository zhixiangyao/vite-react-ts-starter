{
  "name": "vite-react-ts-starter",
  "version": "0.1.0",
  "author": "zhixiangyao <blueskyzx@icloud.com> <https://github.com/zhixiangyao>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhixiangyao/vite-react-ts-starter.git"
  },
  "homepage": "https://github.com/zhixiangyao/vite-react-ts-starter",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "npx eslint --fix"
    ],
    "src/**/*.{js,json,tsx,css,less,scss,html,md}": [
      "npx prettier --write --loglevel warn "
    ],
    "src/**/*.{jsx,tsx,less,postcss,css,scss}": [
      "npx stylelint --fix --cache --cache-location node_modules/.cache/stylelint/"
    ]
  },
  "dependencies": {
    "mobx": "^6.3.3",
    "mobx-react": "^7.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.1",
    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "@types/node": "^16.7.10",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-config": "^5.0.3",
    "@types/react-router-dom": "^5.1.8",
    "@types/styled-components": "^5.1.13",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^7.0.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^4.4.2",
    "typescript-plugin-css-modules": "^3.4.0",
    "vite": "^2.5.3",
    "vite-plugin-windicss": "^1.3.0",
    "windicss": "^3.1.7"
  },
  "license": "MIT",
  "engines": {
    "node": "^12 || ^14 || ^15 || ^16"
  }
}
